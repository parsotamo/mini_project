  "devDependencies": {
    "@fortawesome/fontawesome-free": "^5.15.2",
    "autoprefixer": "^8.6.5",
    "bootstrap": "^5.0.0-beta2",
    "node-sass": "^5.0.0",
    "popper.js": "^1.16.1",
    "postcss-cli": "^7.1.2",
    "stylelint": "^13.12.0"
  }
}
