  "main": "index.js",
  "scripts": {
    "watch:sass": "node-sass scss/main.scss static/css/main.css -w",
    "devserver": "live-server",
    "start": "npm-run-all devserver watch:sass",
    "prefix:css": "postcss --use autoprefixer -r static/css",
